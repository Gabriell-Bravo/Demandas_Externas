{
  "version": 2,
  "builds": [
    {
      "src": "Controle_Demandas/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Controle_Demandas/wsgi.py"
    }
  ],
  "env": {
    "SECRET_KEY": "@secret_key",
    "POSTGRES_URL": "@postgres_url"
  }
}