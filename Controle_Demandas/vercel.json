{
  "version": 2,
  "builds": [
    {
      "src": "Controle_Demandas/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "Controle_Demandas/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "Controle_Demandas/mediafiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Controle_Demandas/wsgi.py"
    }
  ]
}